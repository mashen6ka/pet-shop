{
  "name": "@pet-shop/server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "ts-node src/main.ts",
    "dev:start": "NODE_ENV=development nodemon src/main.ts",
    "dev:migrate": "NODE_ENV=development node ./migrate.js",
    "ci:migrate": "NODE_ENV=ci node ./migrate.js",
    "test:clear": "rm -rf allure-results",
    "test": "npm run test:clear & NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:unit": "npm run test -- --testPathPattern=test/unit",
    "test:integration": "npm run test -- --testPathPattern=test/integration"
  },
  "author": "mashen6ka",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@types/reflect-metadata": "^0.1.0",
    "allure-commandline": "^2.20.1",
    "chance": "^1.1.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "jasmine-allure-reporter": "^1.0.2",
    "jest-allure": "^0.1.3",
    "jest-jasmine2": "^29.3.1",
    "jest-junit": "^15.0.0",
    "lodash": "^4.17.21",
    "pg": "^8.7.3",
    "postgres-migrations": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.19.3",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/chance": "^1.1.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.182",
    "@types/npmlog": "^4.1.4",
    "@types/pg": "^8.6.5",
    "nodemon": "^2.0.19",
    "npmlog": "^6.0.2",
    "npmlog-file": "^0.1.4",
    "ts-jest": "^29.0.3"
  }
}
